  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5a4b81"
      }
    },
    {
      "name": "Strings",
      "scope": "string, constant.character.escape.regexp",
      "settings": {
        "foreground": "#ffcc60",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter, variable.language.arguments",
      "settings": {
        "foreground": "#fd971f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control",
      "scope": "keyword.control, storage.type.class, storage.modifier.java, punctuation.definition.tag, keyword.other.import, punctuation.section.embedded",
      "settings": {
        "foreground": "#f92672",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Language constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#70e0ef",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function definition",
      "scope": "storage.type",
      "settings": {
        "foreground": "#70e0ef",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function, meta.function-call.generic, keyword.other.special-method, entity.other.attribute-name",
      "settings": {
        "foreground": "#a6e22e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator, punctuation.definition.variable, punctuation.separator.key-value",
      "settings": {
        "foreground": "#f92672",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Basic language functions",
      "scope": "support.type, entity.name.type.class, support.function, support.class, keyword.other.typedef",
      "settings": {
        "foreground": "#70e0ef",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Format",
      "scope": "constant.character.format, constant.character.escape",
      "settings": {
        "foreground": "#7a6baa"
      }
    },
    {
      "name": "Properties",
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#7b68ee"
      }
    },
    {
      "name": "Properties quotation mark",
      "scope": "punctuation.support.type.property-name.begin, punctuation.support.type.property-name.end",
      "settings": {
        "foreground": "#98fb98"
      }
    },
    {
      "name": "Properties value",
      "scope": "meta.property-value",
      "settings": {
        "foreground": "#70ddff"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#ff9030",
        "fontStyle": "bold"
      }
    },
    {
      "name": "SQL keyword",
      "scope": "keyword.other.create, keyword.other.sql, keyword.other.DML, keyword.other.LUW, keyword.other.alias",
      "settings": {
        "foreground": "#f92672",
        "fontStyle": "bold"
      }
    }
  ]
}